<div class="profile-wrap">
<div class="user_profile">
<%= image_tag @user.avatar.url(:medium), :class => "user_profile" %>

<div class="profile_name">
  <em class="clicker">Click to edit</em>
<em> Name </em>
<h2 class="user_profile"><%= best_in_place @user, :name%></h2>
<em> Email </em>
<h2 class="user_profile"><%= best_in_place @user, :email %></h2>
<br>
<em>Zipcode</em>
<h2 class="user_profile"><%= best_in_place @user, :zipcode %></h2>
<q class="quote"><%= best_in_place @user, :tagline, :nil => "add a tagline here" %></q>
<span class="user_profile"> Is the proud owner of:
  <% @user.dogs.each do |dog| %> 
<li class="user_profile>"><%= link_to dog.name, dog_path(dog), class: "dog-name" %>, a <%= dog.gender %> <%= dog.breed %> who likes to play with <%= dog.preference %> dogs.</li>
<% end %></span>
</div>
</div>
</div>
